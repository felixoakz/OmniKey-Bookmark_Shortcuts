{
  "manifest_version": 3,
  "name": "Greater Shortcuts",
  "description": "Use >keyword in the address bar to open matching bookmarks.",
  "version": "1.0",
  "permissions": ["bookmarks", "tabs", "notifications"],
  "omnibox": {
    "keyword": ">"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Greater Shortcuts"
  }
}
