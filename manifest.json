{
  "manifest_version": 3,
  "name": "OmniKey - Bookmark Shortcuts",
  "description": "Use the address bar to open bookmarks with custom shortcuts.",
  "version": "1.0",
  "permissions": ["bookmarks", "tabs", "notifications"],
  "omnibox": {
    "keyword": "/"
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "OmniKey - Bookmark Shortcuts"
  }
}
